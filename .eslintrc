{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["plugin:react/recommended", "google", "plugin:prettier/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"ignorePatterns": ["**/*.d.ts"],
	"plugins": ["@typescript-eslint", "react"],
	"rules": {
		"require-jsdoc": "off"
	}
}
